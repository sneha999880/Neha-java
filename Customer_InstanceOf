class Customer {//parent
  
}

class Neha extends Customer {//child
    
}

public class Main {
    public static void main(String[] args) {
        Customer customerObj = new Customer();
        System.out.println("customerObj instanceof Customer: " + (customerObj instanceof Customer));

        Neha nehaObj = new Neha();
        System.out.println("nehaObj instanceof Customer: " + (nehaObj instanceof Customer));
    }
}


